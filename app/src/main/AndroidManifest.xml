<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.svago">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.PickDateActivity"></activity>
        <activity android:name=".SharedPackage.DoneActivity" />
        <activity android:name=".SharedPackage.PaymentActivity" />
        <activity android:name=".FlightPackage.ClassTransferActivity" />
        <activity android:name=".SharedPackage.HomeActivity" />
        <activity android:name=".SharedPackage.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>